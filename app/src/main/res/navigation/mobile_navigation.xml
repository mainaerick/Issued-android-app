<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ticket.issued.ui.Fragment.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_submit"
        android:name="com.ticket.issued.ui.Fragment.SubmitFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_submit" />

    <fragment
        android:id="@+id/navigation_issues"
        android:name="com.ticket.issued.ui.Fragment.IssuesFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_issues" />
    <fragment
        android:id="@+id/account_Fragment"
        android:name="com.ticket.issued.ui.Fragment.AccountFragment"
        android:label="fragment_account_"
        tools:layout="@layout/fragment_account" />
</navigation>